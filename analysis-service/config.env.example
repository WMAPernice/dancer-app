# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# S3 Configuration
S3_BUCKET=dancer-uploads
S3_PREFIX=uploads

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dancer-upload-notifications
# OR use queue name (will auto-resolve URL)
SQS_QUEUE_NAME=dancer-upload-notifications
SQS_WAIT_TIME=20
SQS_VISIBILITY_TIMEOUT=300
SQS_MAX_MESSAGES=10

# S3 Event Notification Settings
EXPECTED_S3_EVENT_SOURCE=aws:s3
EXPECTED_S3_EVENT_PREFIX=ObjectCreated

# Processing Configuration
DOWNLOAD_DIR=./temp_downloads
MAX_WORKERS=2
CLEANUP_AFTER_PROCESSING=true
POLL_INTERVAL_SECONDS=5
MAX_RUNTIME_HOURS=1.0

# Database Configuration (reuse from main backend)
DATABASE_URL=sqlite:///./dancer_uploads.db

# Logging Configuration
LOG_LEVEL=INFO
